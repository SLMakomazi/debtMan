# API Configuration
REACT_APP_API_URL=http://localhost:5000/api

# Environment
NODE_ENV=development

# Authentication
REACT_APP_AUTH_TOKEN_KEY=debt_manager_auth_token

# Feature Flags
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_DEBUG_LOGS=false

# External Services (replace with actual values in .env.local)
REACT_APP_GOOGLE_ANALYTICS_ID=UA-XXXXXXXXX-X
REACT_APP_STRIPE_PUBLIC_KEY=pk_test_XXXXXXXXXXXXXXXXXXXXXXXX

# Session
REACT_APP_SESSION_TIMEOUT=3600000 # 1 hour in milliseconds

# Pagination
REACT_APP_ITEMS_PER_PAGE=10

# URLs
REACT_APP_BASE_URL=http://localhost:3000
REACT_APP_API_TIMEOUT=30000 # 30 seconds

# Logging
REACT_APP_LOG_LEVEL=debug # error, warn, info, debug

# Performance
REACT_APP_PERFORMANCE_TRACKING=true
